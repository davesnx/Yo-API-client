  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.3",
    "chai": "^3.4.0",
    "eslint": "^1.7.1",
    "mocha": "^1.20.1",
    "nodemon": "^1.8.0"
  },
  "dependencies": {
    "superagent": "^1.4.0"
